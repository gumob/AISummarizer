{
  "name": "ai-summarizer",
  "version": "0.0.1",
  "description": "A Chrome extension to manage other extensions",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development --watch",
    "build": "NODE_ENV=production node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=production",
    "start": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development",
    "prettier-check": "prettier --config prettier.config.js --check \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "prettier-fix": "prettier --config prettier.config.js --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "eslint-check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "eslint-fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "type-check": "tsc --noEmit",
    "clean-install-modules": "rm -rf node_modules && pnpm install",
    "test": "jest"
  },
  "keywords": [
    "chrome-extension",
    "ai-summarizer",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/react": "^0.27.10",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@mozilla/readability": "^0.6.0",
    "@plasmohq/storage": "^1.3.0",
    "@types/react-window": "^1.8.8",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "domhandler": "^5.0.3",
    "entities": "^6.0.0",
    "html-dom-parser": "^5.1.1",
    "idb": "^8.0.3",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-window": "^1.8.11",
    "tslog": "^4.9.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
